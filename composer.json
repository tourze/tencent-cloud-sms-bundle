{
  "name": "tourze/tencent-cloud-sms-bundle",
  "description": "腾讯云短信服务模块",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "doctrine/doctrine-bundle": "^2.13",
    "symfony/framework-bundle": "^6.4",
    "symfony/serializer": "^6.4",
    "tencentcloud/sms": "^3.0.1327",
    "tourze/enum-extra": "0.0.*",
    "tourze/easy-admin-attribute": "0.0.*",
    "tourze/doctrine-indexed-bundle": "0.0.*",
    "tourze/doctrine-track-bundle": "0.0.*",
    "tourze/doctrine-user-bundle": "0.0.*",
    "tourze/doctrine-timestamp-bundle": "0.0.*"
  },
  "autoload": {
    "psr-4": {
      "TencentCloudSmsBundle\\": "src"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1"
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1"
  }
